<table>
    <th >DMV details:</th>
    <tr>
        <td>DL Number : </td>
        <td><input class="form-control" name="driverLicense" type="text" class="form-control" id="driverLicense" placeholder="DL number" readonly value="${DMV.driverLicense}"></td>
    </tr>
    <tr>
        <td>status : </td>
        <td><input class="form-control" name="statusOfDL" type="text" class="form-control" id="status" placeholder="staus" readonly value="${DMV.statusOfDL}"></td>
    </tr>
    <tr>
        <td>current year : </td>
        <td><input class="form-control" name="currentYear" type="text" class="form-control" id="type" placeholder="current year" readonly value="${DMV.currentYear}"></td>
    </tr>
    <tr>
        <td>Number of illegal driving cases/challan : </td>
        <td><input class="form-control" name="noOfIllegalDrivingCasesinCurrentYear" type="text" class="form-control" id="vehicleNumber" placeholder="number of challan" readonly value="${DMV.noOfIllegalDrivingCasesinCurrentYear}"></td>
    </tr>
</table>